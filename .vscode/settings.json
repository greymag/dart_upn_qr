{
    "dart.flutterSdkPath": ".fvm/flutter_sdk",
    // Remove .fvm files from search
    "search.exclude": {
      "**/.fvm": true
    },
    // Remove from file watching
    "files.watcherExclude": {
      "**/.fvm": true
    },
    "cSpell.words": [
      "BARJANSKA",
      "CPYR",
      "Lepa",
      "nalog",
      "plačilni",
      "Šubičeva",
      "Univerzalni"
    ],
    "cSpell.ignorePaths": [
      "example/main.dart",
      "test/upn_qr_parser_test.data.dart",
      "test/upn_qr_parser_test.expect.dart",
    ]
}